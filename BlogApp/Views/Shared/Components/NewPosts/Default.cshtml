@model List<Post>
<div class="card mt-4">
    <div class="card-header">
        <h5 class="card-title">New Posts</h5>
    </div>
    <div class="card-body">
        @foreach (var post in Model)
        {
            <div class="mb-3">
                <h6 class="mb-0">
                    <h5><a href="@Url.Action("Details", "StartPage", new { url = post.Url })" class="linkbutton p-0";">@post.Title</a></h5>
                </h6>
            </div>
        }
    </div>
</div>